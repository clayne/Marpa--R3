# A 2nd Makefile, to keep developer targets from cluttering
# the EU::MM Makefile, which is for installation

.PHONY: dummy debug license meta

dummy:
	echo No default target

debug:
	make clean
	perl Makefile.PL OPTIMIZE=-g
	make OPTIMIZE=-g

license:
	perl etc/check_license.pl

libmarpa/dists.time-stamp:
	(cd libmarpa; make dists)

engine/read_only.time-stamp: libmarpa/dists.time-stamp
	sh -x etc/libmarpa_from_tar.sh libmarpa/libmarpa-8.4.0.tar.gz

meta: engine/read_only.time-stamp
	make
